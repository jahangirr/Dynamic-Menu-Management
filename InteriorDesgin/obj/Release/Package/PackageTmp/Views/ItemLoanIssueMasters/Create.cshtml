            }
        });


        $('#ItemDescription').change(function () {

            $.ajax({
                url: '@Url.Action("GetItemDetialByItemName", "SprOrIndentRegisters")',
                type: "GET",
                dataType: "JSON",
                data: {
                    ItemName: $('#ItemDescription').val()
                },
                success: function (data) {

                    $('#ItemCode').val(data.ItemCode);
                    $('#ItemCodeShow').val(data.ItemCode);

                },
